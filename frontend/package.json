{
  "name": "@poo-tracker/frontend",
  "version": "1.0.0",
  "description": "Frontend for Poo Tracker",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "eslint . --ext .ts,.tsx && vitest run",
    "test:watch": "eslint . --ext .ts,.tsx && vitest --watch",
    "test:coverage": "eslint . --ext .ts,.tsx && vitest run --coverage",
    "test:ui": "eslint . --ext .ts,.tsx && vitest --ui",
    "clean": "rm -rf dist node_modules"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.515.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.3",
    "sonner": "^2.0.5",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "3.2.3",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.5",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
